	            titleVisible: false,
	            content: ['<div class="pop_msg" style="position:realtive;" ><table><tr><td>', h, '</td></tr></table><i style="position:absolute;width:16px;height:20px;right:5px;top:5px;"></i></div>'].join(""),
	            contentStyle: {
	                "background-color": "transparent"
	            },
	            borderStyle: {
	                border: "0px"
	            },
	            closeCls: "hide"
	        });
	        g.getContent().find("i").click(function(j) {
	            if (i) {
	                clearTimeout(i);
	            }
	            g.destory();
	        });
	        g.show();
	        var i = setTimeout(function() {
	            g.destory();
	        },
	        1500);
	    };
	});
