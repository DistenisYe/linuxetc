	raiders.use("by_qw",
	function() {
	    var a = {
	        format: function(f, c) {
	            c = c || "yyyy-MM-dd";
	            var g = f.getFullYear().toString(),
	            e = {
	                M: f.getMonth() + 1,
	                d: f.getDate(),
	                h: f.getHours(),
	                m: f.getMinutes(),
	                s: f.getSeconds()
	            };
	            c = c.replace(/(y+)/ig,
	            function(h, d) {
	                return g.substr(4 - Math.min(4, d.length));
	            });
	            for (var b in e) {
	                c = c.replace(new RegExp("(" + b + "+)", "g"),
	                function(h, d) {
	                    return (e[b] < 10 && d.length > 1) ? "0" + e[b] : e[b];
	                });
	            }
	            return c;
	        },
	        plusDay: function(c, b) {
	            var e = 24 * 60 * 60 * 1000;
	            return new Date(c.getTime() + b * e);
	        }
	    };
	    this.DateH = a;
	}); (function() {
	    var a = raiders.use("by_qw.DateH");
	    for (var b in a) {
	        Date.prototype[b] = raiders.helper.methodize(a[b]);
	    }
	})();
	raiders.use("_class",
	function(a) {
	    var c = Array.prototype.slice;
	    var b = a.helper;
	    this.Event = a.helper.mixPrototype(function() {
	        this._eventList = {};
	        this._bind_self_event_();
	    },
	    {
	        _getOne: function(d) {
	            return this._eventList[d] || (this._eventList[d] = []);
	        },
	        bind: function(e, g, f) {
	            if (typeof e === "object") {
	                for (var h in e) {
	                    this.bind(h, e[h]);
	                }
	            } else {
	                if (f) {
	                    g = b.proxy(g, f);
	                }
	                var d = this._getOne(e);
	                if (d.fired) {
	                    g.apply(f, d.args);
	                    return this;
	                }
	                this._getOne(e).push(g);
	                return this;
	            }
	        },
	        unbind: function(d, e) {
	            var f = this._getOne(d);
	            if (e) {
	                var g = f.indexOf(e);
	                if (g > -1) {
	                    f.splice(g, 1);
	                }
	            } else {
	                f.length = 0;
	            }
	        },
	        trigger: function(f) {
	            var e = c.call(arguments, 1);
	            var g = this;
	            var d;
	            this._getOne(f).forEach(function(h) {
	                d = h.apply(g, e);
	                if (d === false) {
	                    return true;
	                }
	            });
	            return d;
	        },
	        once: function(d, g, e) {
	            var f = this;
	            var h = function() {
	                g.apply(e || f, arguments);
	                f.unbind(d, h);
	            };
	            f.bind(d, h);
	            return h;
	        },
	        delay: function(e, g) {
	            var d = c.call(arguments, 2);
	            var f = this;
	            return setTimeout(function() {
	                f.trigger.apply(f, e.concat(d));
	            },
	            g || 10);
	        },
	        triggerReady: function(e) {
	            var f = this._getOne(e);
	            var d = Array.prototype.slice.call(arguments, 1);
	            f.forEach(function(h, g) {
	                if (typeof h != "function") {
	                    return null;
	                }
	                return h.apply(null, d);
	            });
	            f.fired = true;
	            f.args = d;
	        },
	        _bind_literal_event_: function(d, e) {
	            this.bind(d, e);
	        },
	        _bind_self_event_: function() {
	            for (var d in this) {
	                var e = /^_event_(.+)_$/.exec(d);
	                if (!e) {
	                    continue;
	                }
	                this._bind_literal_event_(e[1], this[d]);
	            }
	        }
	    });
	    this.Event.prototype.fire = this.Event.prototype.trigger;
	    this.Event.prototype.on = this.Event.prototype.bind;
	});
	raiders.use("event",
	function() {
	    raiders.helper.outMethod(this, new raiders._class.Event("base class"));
	});
