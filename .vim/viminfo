	        dialogs: {},
	        showedDialog: null,
	        addDiaglog: function(f) {
	            this.dialogs[f.id] = f;
	        },
	        closeAll: function() {
	            var f = this.dialogs;
	            for (var g in f) {
	                if (f[g] && f[g].out) {
	                    f[g].out();
	                } else {
	                    delete f[g];
	                }
	            }
	        },
	        showAll: function() {},
	        removeAll: function() {},
	        setShowedDialog: function(f) {
	            this.showedDialog = f;
	        },
	        getShowedDialog: function() {
	            return this.showedDialog;
	        }
	    };
	    var e = 0,
	    b = "SHOW",
	    d = "HIDE";
