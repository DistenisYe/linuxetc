<        var n = $('<div>							<div class="js_body"></div>						 	<div class="ke-dialog-footer">						 	<span title="添加" class="ke-button-common ke-button-outer ke-dialog-yes">								<input type="button" value="确定" class="ke-button-common ke-button"></span>							<span title="取消" class="ke-button-common ke-button-outer ke-dialog-no">								<input type="button" value="取消" class="ke-button-common ke-button">							</span>							</div>						</div>');
