	        o.title = function(w) {
	            f.title.text(w);
	        };
	        if (f.hideTitle) {
	            o.getCloseBtn().hide();
	        }
	        o.id = e++;
	        o.out = o.close;
	        a.addDiaglog(o);
	        return o;
	    };
